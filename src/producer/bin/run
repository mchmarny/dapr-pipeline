#!/bin/bash

set -o errexit
set -o pipefail

DIR="$(dirname "$0")"
. "${DIR}/config"

go mod tidy
go mod vendor

dapr run --app-id $PROVIDER_APP_NAME \
         --app-port $PORT \
         --protocol http \
         --max-concurrency 1 \
         --port $DAPR_HTTP_PORT \
         go run search.go handler.go main.go
