#!/bin/bash

set -o errexit
set -o pipefail

export TW_CONSUMER_KEY="${TW_CONSUMER_KEY:-test-consumer-key}"
export TW_CONSUMER_SECRET="${TW_CONSUMER_SECRET:-test-consumer-secret}"

go mod tidy
go mod vendor

dapr run --app-id provider \
         --app-port 8080 \
         --protocol http \
         --max-concurrency 1 \
         --port 3500 \
         go run search.go handler.go main.go
